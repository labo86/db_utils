{
  "name": "labo86/db_utils",
  "description": "Una nueva asombrosa biblioteca Composer",
  "authors": [
    {
      "name": "Edwin Rodríguez-León",
      "role": "Developer"
    }
  ],
  "license": "MIT",
  "minimum-stability": "stable",
  "require": {
    "php": "^7.4",
    "ext-pdo": "*",
    "ramsey/uuid": "^4.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5",
    "mikey179/vfsstream": "^1.6"
  },
  "autoload": {
    "psr-4": {
      "labo86\\db_utils\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "test\\labo86\\db_utils\\": "test/"
    }
  },
  "archive": {
    "exclude": [
      "/.git",
      "/.idea",
      "/examples",
      "/scripts",
      "/test",
      "/.gitignore",
      "/.phpunit.result.cache",
      "/phpunit.xml.dist",
      "/README.md",
      "/.travis.yml"
    ]
  }
}
